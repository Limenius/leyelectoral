{% extends "layout.html" %}
{% block content %}
<style type="text/css" media="screen">
#holder {
  margin: -350px 0 0 -350px;
  width: 700px;
  height: 700px;
  left: 50%;
  position: absolute;
  top: 50%;
}
</style>
<div id="holder"></div>
<script>



$(document).ready(function(){
    {% for party, results in votes %}
    DataStore.add({
        label: "{{parties[party]|raw}}",
        amount: parseInt({{results}}, 10),
    });
    {% endfor %}
    DataStore.trigger("redraw");
});

</script>
{% endblock %}
